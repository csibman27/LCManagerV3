{{> menu active="dependency"}}


<div class="grid-cols-8 text-center space-x-5 mb-5">
  <span class="bg-blue-100 text-blue-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-blue-400 border border-blue-400">Openstack</span>
  <span class="bg-gray-100 text-gray-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-gray-400 border border-gray-500">Proxmox</span>
  <span class="bg-red-100 text-red-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-red-400 border border-red-400">Vmware ESXI</span>
  <span class="bg-green-100 text-green-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-green-400 border border-green-400">Solaris R720-17</span>
  <span class="bg-yellow-100 text-yellow-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-yellow-300 border border-yellow-300">Solaris X4450-2</span>
  <span class="bg-indigo-100 text-indigo-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-indigo-400 border border-indigo-400">R420-5 LDAP Internal consumer</span>
  <span class="bg-purple-100 text-purple-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-purple-400 border border-purple-400">Solaris R720-21</span>
  <span class="bg-pink-100 text-pink-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-pink-400 border border-pink-400">Solaris R720-20</span>
</div>
<div class="h-screen">
  <div class="grid grid-cols-3 grid-rows-3 gap-4 h-5/6 w-4/5 mx-auto">
    {{#each servers }}
      <div class="bg-green-100 row-span-1  col-span-1">
            <span class="bg-blue-100 text-blue-800 text-me font-medium me-2 px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-blue-400 border border-blue-400">
              {{service}}
            </span>

        <a href="/dependency/{{_id}}" class="button">
          <i class="fa-solid fa-server fa-xl"></i>
          Number: {{{server.services.length}}}
          <div class="bg-green-100 row-span-1  col-span-1">

          </div>
        </a>
      </div>
    {{/each}}
    <p>ServiceName</p>
    {{#each server.services}}
      {{serviceName}}
    {{/each}}
    {{log server.services.length}} hello
  </div>
</div>


<div>
  {{#each server.services}}
    <div class="row">
      <div class="col-md-12">

        <p>{{serviceName}}</p>

      </div>
    </div>
  {{/each}}

  <p>There are no documents available at this time</p>

</div>




{{> footer }}